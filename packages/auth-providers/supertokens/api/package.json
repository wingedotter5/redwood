    "@babel/cli": "7.23.4",
    "@babel/core": "^7.22.20",
    "@redwoodjs/api": "6.0.7",
    "@types/jsonwebtoken": "9.0.5",
    "jest": "29.7.0",
    "typescript": "5.3.2"
  },
  "peerDependencies": {
    "supertokens-node": "16.6.5"
  },
